{
  "name": "agentic-security-cdk",
  "version": "0.1.0",
  "bin": {
    "agentic-security-cdk": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "cdk-test": "jest",
    "lambda-build": "cd ../lambda && GOOS=linux GOARCH=amd64 go build -o dist/bootstrap handler.go && cd ../cdk",
    "lambda-test": "cd ../lambda && go test && cd ../cdk",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "format": "prettier --write 'lib/**/*.ts' 'bin/**/*.ts'"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "aws-cdk-lib": "^2.130.0",
    "constructs": "^10.3.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20"
  }
}
